
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h1>
            @ViewBag.companyName
        </h1>
    </div>
    <div>
        <ul>
            @foreach (var item in ViewBag.mylist)
            {
                <li>@item</li>
            }
        </ul>
    </div>
    <div>
        <ul>
            @foreach (var emp in ViewBag.myEmpList)
            {
                <li>Id = @emp.Id , Name = @emp.name, salary = @emp.salary</li>
                
            }
        </ul>
    </div>
</body>
</html>
